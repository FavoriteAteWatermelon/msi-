<!--pages/barcode/barcode.wxml-->
 <view class="container">
   <canvas class="canvas-code"canvas-id="canvas" style="background:#fff;width: 200px;height: 200px;border:1px solid red"/>
   <form bindsubmit='generate' style="margin-top:20px">
        <input type='text'  placeholder="請輸入內容" class="inputContent" name='content' ></input>
       <button form-type='submit' class="barcode">生成二維碼</button>
    </form>
    <view class="modal-button" style="margin-top:20px">
      <button bindtap='savePic' class="barcode">保存到手機</button>  </view>
  </view>
